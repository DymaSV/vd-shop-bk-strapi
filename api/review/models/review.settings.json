{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "name": "Review"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "IdReview": {
      "type": "uid"
    },
    "Title": {
      "type": "string"
    },
    "Text": {
      "type": "richtext"
    },
    "Star": {
      "type": "integer"
    },
    "Date": {
      "type": "datetime"
    },
    "product": {
      "via": "reviews",
      "model": "product"
    },
    "user": {
      "via": "reviews",
      "plugin": "users-permissions",
      "model": "user"
    },
    "review_images": {
      "via": "review",
      "collection": "review-images"
    }
  }
}
