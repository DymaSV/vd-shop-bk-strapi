{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "name": "Review",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "IdReview": {
      "type": "uid"
    },
    "Title": {
      "type": "string"
    },
    "Text": {
      "type": "richtext"
    },
    "Star": {
      "type": "integer"
    },
    "Date": {
      "type": "datetime"
    },
    "product": {
      "model": "product",
      "via": "reviews"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "reviews"
    },
    "review_images": {
      "collection": "review-images",
      "via": "review"
    },
    "IsActive": {
      "type": "boolean",
      "default": false
    }
  }
}
